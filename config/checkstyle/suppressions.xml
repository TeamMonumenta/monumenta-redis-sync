<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
	"-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
	"https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- These files need to exactly match what's in Paper, ignore style warnings here -->
	<suppress files=".*[\\/]com/destroystokyo/paper/event/player/.*java" checks=".*" />

	<suppress files=".*[\\/]VersionAdapter_.*java"
			  checks="TypeName"
			  message=".*Type name.*must match pattern.*"/>
	<suppress files=".*[\\/]BungeeListener\.java"
			  checks="RegexpSinglelineJava"
			  message="Event handlers must specify ignoreCancelled"/>
</suppressions>
